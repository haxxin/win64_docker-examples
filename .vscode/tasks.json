{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "service build",
      "type": "shell",
      "command": "docker compose build"
    },
    {
      "label": "service up",
      "type": "shell",
      "command": "docker compose up -d"
    },
    {
      "label": "service up (create)",
      "type": "shell",
      "command": "docker compose up -d --force-recreate"
    },
    {
      "label": "service down",
      "type": "shell",
      "command": "docker compose down"
    },
    {
      "label": "service stop",
      "type": "shell",
      "command": "docker compose stop"
    },
    {
      "label": "service logs",
      "type": "shell",
      "command": "docker compose logs --since=24h"
    }.
    {
      "label": "service attach",
      "type": "shell",
      "command": "docker -exec it nvidia-toolkit-1 sh"
    }
  ]
}
